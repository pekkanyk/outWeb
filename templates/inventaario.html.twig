<html xmlns="http://www.w3.org/1999/xhtml" >
    <head lang="en">
        {{ include('fragments/_head.html.twig') }}
    </head>
    <body> {% block body %}
        <header>
            {{ include('fragments/_top_navi.html.twig') }}   
        </header>
        <div class="container">
            <div class="row">
             <div class="col-md-12">
                    <table class="table taulukot">
                        <caption>Jakauma</caption>
                        <thead>
                        <tr>
                            {% for key,count in isotNumerot %}
                                <th><a class="active" href="/invis/{{ key }}">{{ key }} ({{ count }}) kpl</a></th>
                            {% endfor %}
                            
                        </tr>
                        </thead>
                        
                            {% for key,vika in hyllypaikkaCount %}
                                <tr>
                                {% set tokaVikaNro = key %}
                                {% for key,lukema in vika %}
                                    <td><a class="active" href="/invis/{{ tokaVikaNro }}{{ key }}">{{ tokaVikaNro }}{{ key }} (<b>{{ lukema }}</b> kpl)</a></td>
                                {% endfor %}
                                </tr>
                            {% endfor %}
                    </table>
                </div>   
            </div>
            
            
        </div>
            {% endblock %}
    </body>